# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your-supabase-project-url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key

# Next.js Configuration
NEXT_PUBLIC_APP_URL=http://localhost:3000
NODE_ENV=development

# Stripe Configuration
STRIPE_SECRET_KEY=sk_test_your-stripe-secret-key
STRIPE_WEBHOOK_SECRET=whsec_your-webhook-secret

# Dodo Payment Configuration
# Note: Some providers only use API Key (no separate secret key)
DODO_PAYMENT_API_KEY=your-dodo-payment-api-key
DODO_PAYMENT_SECRET_KEY=your-dodo-payment-secret-key  # Optional - only if Dodo provides it
DODO_PAYMENT_ENVIRONMENT=sandbox
DODO_PAYMENT_WEBHOOK_SECRET=your-dodo-webhook-secret  # Found in webhook settings, not API settings

# Email Configuration (Resend)
RESEND_API_KEY=re_your-resend-api-key
RESEND_DOMAIN=your-domain.com

# File Upload Configuration
UPLOAD_DIR=./uploads
MAX_FILE_SIZE=10485760